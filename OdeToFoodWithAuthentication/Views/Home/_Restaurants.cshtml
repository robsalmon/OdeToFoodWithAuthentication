@model IEnumerable<OdeToFoodWithAuthentication.Models.RestaurantListViewModel>
@using OdeToFoodWithAuthentication.Models

<div id="restaurantList">
    @foreach (RestaurantListViewModel item in Model)
    {
        <div>
            <h4>@item.Name</h4>
            <div>@item.City, @item.Country</div>
            <div>Reviews @item.CountOfReviews</div>
            <hr />
        </div>
    }
</div>